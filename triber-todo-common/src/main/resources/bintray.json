{
  "package": {
    "name": "triber-todo-common",
    "repo": "maven",
    "subject": "triberraar",
    "vcs_url": "https://github.com/triberraar/triber-todo-common",
    "github_use_tag_release_notes": true,
    "licenses": [
      "MIT"
    ],
    "public_download_numbers": false,
    "public_stats": false
  },
  "version": {
    "name": "@project.version@",
    "vcs_tag": "@project.version@",
    "released": "@maven.build.timestamp@"
  },
  "files": [
    {
      "includePattern": "target/(.*\.jar)",
      "uploadPattern": "be/tribersoft/triber-todo-common/@project.version@/$1"
    },
    {
      "includePattern": "pom.xml",
      "uploadPattern": "be/tribersoft/triber-todo-common/@project.version@/triber-todo-common-@project.version@.pom"
    }
  ],
  "publish": true
}